{
  "name": "botcomet-bot",
  "type": "module",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p . && babel src --out-dir dist --extensions \".ts,.js\" --source-maps",
    "lint": "eslint src --ext .ts",
    "start": "node dist/index.js",
    "build:start": "npm run build && npm run start",
    "dev": "BETA=true nodemon --watch src --ext ts --exec \"npm run build:start\"",
    "prod:commands": "node dist/deploy_commands.js",
    "dev:commands": "BETA=true node dist/deploy_commands.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-plugin-add-import-extension": "^1.6.0",
    "eslint": "^8.31.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  }
}
